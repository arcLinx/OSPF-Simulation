all: serverA.o serverB.o serverC.o serverD.o client.o
    gcc -o server -g server.o 

serverA.o: serverA.c 
    gcc -g -c -Wall serverA.c

	serverB.o: serverB.c 
    gcc -g -c -Wall serverB.c

serverC.o: serverC.c 
    gcc -g -c -Wall serverC.c

serverD.o: serverD.c 
    gcc -g -c -Wall serverD.c
    
client.o: client.c 
    gcc -g -c -Wall client.c

clean:
    rm -f *.o
